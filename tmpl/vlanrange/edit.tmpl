<tmpl_include name="_header.tmpl">

    <form name="form1" method="POST">
        <input type=hidden name="id" value=<tmpl_var name="id">>
        <table>
            <tr>
                <td>Name: </td>
                <td><input type="text" value="<tmpl_var name=name>" name="name"></input></td>
            </tr>
            <tr>
                <td>Start: </td>
                <td><input type="text" size=4 maxlength=4 value="<tmpl_var name=start>" name="start"></input></td>
            </tr>
            <tr>
                <td>End: </td>
                <td><input type="text" size=4 maxlength=4 value="<tmpl_var name=end>" name="end"></input></td>
            </tr>
            <tr>
                <td>Description: </td>
                <td><textarea cols="40" rows="6" name="description"><tmpl_var name=description></textarea></input></td>
            </tr>
            <tr>
                <td><input type="submit" name="submit" value="submit"></td>
            </tr>
        </table>
    </form>
    <br>

